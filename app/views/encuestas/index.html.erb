<h1>Listing encuestas</h1>

<table>
  <thead>
    <tr>
      <th>Cat modulo</th>
      <th>Cat hora</th>
      <th>Fechatramite</th>
      <th>Nombre</th>
      <th>Edad</th>
      <th>Telefono</th>
      <th>Procesada</th>
      <th>Observaciones</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @encuestas.each do |encuesta| %>
      <tr>
        <td><%= encuesta.cat_modulo %></td>
        <td><%= encuesta.cat_hora %></td>
        <td><%= encuesta.fechaTramite %></td>
        <td><%= encuesta.nombre %></td>
        <td><%= encuesta.edad %></td>
        <td><%= encuesta.telefono %></td>
        <td><%= encuesta.procesada %></td>
        <td><%= encuesta.observaciones %></td>
        <td><%= link_to 'Show', encuesta %></td>
        <td><%= link_to 'Edit', edit_encuesta_path(encuesta) %></td>
        <td><%= link_to 'Destroy', encuesta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Encuesta', new_encuesta_path %>
